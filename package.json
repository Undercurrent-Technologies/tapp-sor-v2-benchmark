    "poc:apt-usdc-large": "node yens-algorithm-poc.js APT USDC 100000 --k=10",
    "poc:apt-usdc-small": "node yens-algorithm-poc.js APT USDC 100 --k=5",
    "poc:eth-usdc": "node yens-algorithm-poc.js ETH USDC 100 --k=5",
    "poc:verbose": "node yens-algorithm-poc.js APT USDT 10000 --k=5 --verbose",
    "poc:k10": "node yens-algorithm-poc.js APT USDT 10000 --k=10",
    "poc:max-hops-2": "node yens-algorithm-poc.js APT USDT 10000 --k=5 --max-hops=2",
    "poc:max-hops-5": "node yens-algorithm-poc.js APT USDT 10000 --k=5 --max-hops=5"
