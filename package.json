{
  "name": "sor-phase2-poc",
  "version": "1.0.0",
  "description": "SOR Phase 2 POC - Yen's K-Shortest Paths Algorithm with real pool data",
  "main": "yens-algorithm-poc.js",
  "scripts": {
    "analyze": "node analyze-pools.js",
    "test:limits": "node test-phase1-limits.js",
    "benchmark": "node benchmark.js",
    "benchmark:apt-usdt": "node benchmark.js APT USDT 10000",
    "benchmark:apt-usdc": "node benchmark.js APT USDC 10000",
    "benchmark:large": "node benchmark.js APT USDT 100000",
    "benchmark:3hops": "node benchmark.js APT USDT 10000 --max-hops=3",
    "phase1": "node phase1-dfs-poc.js",
    "phase1:apt-usdt": "node phase1-dfs-poc.js APT USDT 10000",
    "phase1:3hops": "node phase1-dfs-poc.js USDT USDC 10000 --max-hops=3",
    "poc": "node yens-algorithm-poc.js",
    "poc:apt-usdt": "node yens-algorithm-poc.js APT USDT 10000 --k=5",
